{
  "name": "react-magic-card",
  "version": "2.0.7",
  "description": "React Component library for images gallery using the framer motion animation.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/su-pull/react-magic-card",
    "directory": "react-magic-card/src"
  },
  "scripts": {
    "compile": "npx tsc && npm run copy-css",
    "copy-css": "npm run copy-css1 && npm run copy-css2 && npm run copy-css3",
    "copy-css1": "cp src/CircleRotation/styles.module.css dist/CircleRotation/styles.module.css",
    "copy-css2": "cp src/StraightInfinity/styles.module.css dist/StraightInfinity/styles.module.css",
    "copy-css3": "cp src/common/DetailImage/styles.module.css dist/common/DetailImage/styles.module.css",
    "test": "test"
  },
  "keywords": [
    "react",
    "component",
    "animation",
    "images",
    "gallery",
    "circle",
    "infinity"
  ],
  "license": "MIT",
  "peerDependencies": {
    "framer-motion": "^11.3.21"
  },
  "devDependencies": {
    "typescript": "^5.1.3",
    "@types/react": "^18.2.12",
    "@types/css-modules": "^1.0.2"
  }
}
